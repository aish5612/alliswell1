    stud_id int,
    stud_name varchar(255),
    stud_address varchar(255),
    stud_marks varchar(255)
    );


insert into student_info
    values
    (1, 'Murtaza','Pune', 89);


 insert into student_info
    values
    (2, 'Mrunmayee','Hadapsar', 95),
    (3, 'Subrat','Nainital', 97),
    (4, 'Kaushik','Nanded', 92),
    (5, 'Shraddha','Manjri', 86),
    (6, 'Abdullah','Kalyan', 82);


select * from student_info;


alter table student_info
    add stud_college varchar(255) default 'SAE';


alter table student_info
    drop column stud_college;


select stud_name from student_info;


select stud_marks from student_info where stud_marks>90;


select stud_marks from student_info where stud_marks>80 and stud_marks<90;


rename table student_info to stud;
select * from stud;


alter table stud
rename column stud_id to stud_rollno;


alter table stud
modify column stud_address varchar(200);

create view view1 as
    select stud_name, stud_marks
    from stud
    where stud_marks>90;

select * from view1;


create index index1
    on stud(stud_name);

show index from stud;
